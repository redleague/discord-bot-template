{
    "name": "discord-bot-template",
    "version": "1.0.0",
    "description": "",
    "main": "./build/app.js",
    "type": "module",
    "scripts": {
        "build": "npm run lint && npm run compile",
        "compile": "tsc --build tsconfig.json",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "start": "node -r dotenv/config dist/app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/redleague/discord-bot-template.git"
    },
    "author": "Rajat Mishra <hydrox127@gmail.com>",
    "bugs": {
        "url": "https://github.com/redleague/discord-bot-template/issues"
    },
    "homepage": "https://github.com/redleague/discord-bot-template#readme",
    "eslintConfig": {
        "extends": "@hazmi35/eslint-config/typescript",
        "ignorePatterns": "build/*",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2022
        }
    },
    "dependencies": {
        "discord.js": "^14.0.0-dev.1653091711-fdeac9d",
        "pino": "^7.11.0",
        "pino-pretty": "^7.6.1"
    },
    "devDependencies": {
        "@hazmi35/eslint-config": "^8.3.0",
        "@types/node": "^17.0.35",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.16.0",
        "typescript": "^4.6.4"
    },
    "engines": {
        "node": ">=16.7.0",
        "npm": ">=7.0.0"
    }
}
